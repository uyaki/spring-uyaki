<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uyaki.web.mapper.UserMapper">
    <select id="getUserList" resultType="com.uyaki.web.model.vo.UserVO">
        SELECT id, name, created_at, updated_at, is_enable, is_deleted
        FROM user
    </select>
</mapper>